# Redirect URL: Where user is redirected after payment
MOMO_REDIRECT_URL=http://localhost:3000/planning/pricing/success

# SeePay Payment Configuration
# Get these from SeePay after creating a merchant account
# Environment: 'sandbox' for testing, 'production' for live
SEPAY_ENV=sandbox
SEPAY_MERCHANT_ID=your_merchant_id
SEPAY_SECRET_KEY=your_merchant_secret_key
# SeePay API Token for Transaction API
# Create API Token at: Company Settings > API Access > + Add API
# See: https://docs.sepay.vn/tao-api-token.html
SEPAY_API_TOKEN=your_api_token
# SeePay API Base URL (usually https://my.sepay.vn/userapi)
SEPAY_API_BASE_URL=https://my.sepay.vn/userapi
# URLs for payment callbacks
SEPAY_SUCCESS_URL=http://localhost:3000/planning/pricing/success
SEPAY_ERROR_URL=http://localhost:3000/planning/pricing
SEPAY_CANCEL_URL=http://localhost:3000/planning/pricing

